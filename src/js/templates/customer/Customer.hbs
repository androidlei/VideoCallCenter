{{#each customers}}
    <div id="customers{{sid}}"
         class="{{styleName}}"
         onclick="{{onActive}}">
        {{#if isWaiting}}
            <div class="txt-video-call-center-waiting">
                <img class="txt-call-video-rotate" src="data:image/png;base64," />
                <span class="txt-video-call-center-text-tip">正在建立连接，请稍等...</span>
            </div>
        {{else}}
            <div class="txt-video-call-center-customer-header">
                <span class="txt-video-call-center-text-wrap txt-margin-l10 txt-color-white txt-font-14">{{showName}}</span>
                <span id="txtVideoCallCenterTimer{{sid}}" class="txt-color-white txt-font-14 txt-margin-l20">{{wTimer}}</span>
            </div>
            <div class="txt-video-call-center-flex"></div>
            <div class="txt-video-call-center-customer-footer">
                {{#if isRequestTalking}}
                        <div id="txtVideoCallCenterRequestTalk{{sid}}"
                             class="txt-margin-l10 txt-video-call-center-customer-talk-btn"
                             data-sid="{{sid}}"
                             data-cid="{{cid}}"
                             data-extra="{{extra}}"
                             data-showName="{{showName}}"
                        >{{cutDownTime}}</div>
                {{else if isTalking}}
                    <span class="txt-margin-l10 txt-color-white txt-font-em">正在通话中...</span>
                {{/if}}
                <div class="txt-video-call-center-flex"></div>
                <div class="txt-video-call-center-customer-operation-layout">
                    <div id="txtVideoCallCenterTalk{{sid}}" class="{{opClassName}} txt-margin-r10"
                         data-sid="{{sid}}"
                         data-cid="{{cid}}"
                         data-extra="{{extra}}"
                         data-showName="{{showName}}"
                    ></div>
                    <div id="txtVideoCallCenterEndCall{{sid}}" class="txt-video-room-end-call-btn txt-margin-r10"
                         data-sid="{{sid}}"
                         data-cid="{{cid}}"
                         data-extra="{{extra}}"
                         data-stream="{{streamId}}"
                    ></div>
                </div>
            </div>
        {{/if}}
    </div>
{{/each}}